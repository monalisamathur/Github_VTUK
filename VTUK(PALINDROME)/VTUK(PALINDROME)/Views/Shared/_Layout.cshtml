<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<title>@ViewBag.Title - My ASP.NET Application</title>*@

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/Site.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/js/jquery-3.6.0.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/js/JavaScript.js"></script>

</head>


<body>

    <div class="container" id="form-content">
        @RenderBody()

        <table class="table text-center">
            <tr>
                <td><img class="img-thumbnail" src="~/Content/images/JS Image .png"> </td>
                <td><input type="text" class="form-control rounded-pill" id="jscheck"></td>
                <td> &nbsp;</td>
            </tr>
            <tr>
                <td><img class="img-thumbnail" src="~/Content/images/SQL logo.png"></td>
                <td><input type="text" class="form-control rounded-pill" id="sqlcheck"></td>
                <td> <button type="button" class="btn btn-sm btn-outline-success rounded-sm" id="sqlbutton">Go</button> </td>
            </tr>

            <tr>
                <td><img class="img-thumbnail" src="~/Content/images/c.png"></td>
                <td><input type="text" class="form-control rounded-pill" id="cscheck"></td>
                <td> <button type="button" class="btn btn-sm btn-outline-success rounded-sm" id="csbutton">Go</button> </td>
            </tr>

        </table>
     

        </div>
        <footer class="fixed-bottom">
            <img class="rounded mx-auto d-block" id="footerimg" src="~/Content/images/logo_vtuk.png">
        </footer>



</body>
</html>